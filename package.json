    "node-nmap": "^4.0.0",
    "vue": "^3.4.27",
    "vue-toast-notification": "^3.1.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "nodemon": "^3.1.0",
    "opener": "^1.5.2",
    "vite": "^4.0.0"
  },
  "keywords": [
