{
  "name": "nmap-scanner",
  "version": "0.1.0",
  "description": "Connecting to the NMap application locally and displaying it on a webpage",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "echo \"Error: no test specified\" && exit 1",
